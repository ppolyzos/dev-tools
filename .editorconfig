# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = true
charset = utf-8
trim_trailing_whitespace = true

[*]
end_of_line = lf
insert_final_newline = true
indent_style = space
indent_size = 2

[VERSION]
insert_final_newline = false

[Makefile]
indent_style = space

[*.yml]
indent_size = 2

[*.conf]
indent_size = 2


[DOCKER_REPOSITORY]
insert_final_newline = false

[DOCKER_TAG_LATEST]
insert_final_newline = false

[BLACKLIST]
insert_final_newline = false